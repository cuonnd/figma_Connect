{
  "name": "sampleapp",
  "version": "1.0.0",
  "description": "Sample app with Figma MCP integration",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "figma:setup": "chmod +x figma-mcp-quick.sh && ./figma-mcp-quick.sh",
    "figma:setup-global": "chmod +x setup-figma-mcp-global.sh && ./setup-figma-mcp-global.sh",
    "figma:test": "npx @sethdouglasford/mcp-figma@latest --help",
    "figma:init": "npm run figma:setup-global && npm run figma:setup",
    "team:setup": "chmod +x team-setup.sh && ./team-setup.sh"
  },
  "dependencies": {
    "@sethdouglasford/mcp-figma": "latest",
    "dotenv": "^17.2.1",
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  }
}
